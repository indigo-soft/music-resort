{
  "name": "root/music.local",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Root\\MusicLocal\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=8.4",
      "wapmorgan/mp3info": "^0.1.1",
      "symfony/console": "^7.0",
      "symfony/finder": "^7.0",
      "symfony/filesystem": "^7.0",
      "symfony/var-dumper": "^7.3",
      "james-heinrich/getid3": "^1.9",
      "symfony/string": "^7.3",
      "symfony/translation-contracts": "^3.6"
  },
    "require-dev": {
        "pestphp/pest": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
  }
}
